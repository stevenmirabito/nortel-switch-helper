{
  "name": "Nortel Switch Helper",
  "description": "Modernizes the web UI of a Nortel BES1020-48T-PWR switch.",
  "version": "0.1",
  "permissions": [
    "tabs",
    "http://*/*",
    "file://*/*"
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },
    "default_title": "Nortel Switch Helper"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "images/*.svg",
    "css/nortel.css",
    "bower_components/font-awesome/css/font-awesome.min.css",
    "bower_components/font-awesome/fonts/*",
    "content/menu.html"
  ],
  "manifest_version": 2
}